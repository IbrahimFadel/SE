let x = 1;

while(x < 101) {
  if(x % 5 == 0 && x % 3 == 0) {
    print("Fizz buzz:", x);
  } else if(x % 3 == 0) {
    print("Fizz:", x);
  } else if(x % 5 == 0) {
    print("Buzz:", x);
  } else {
    print(x);
  }
  x = x + 1;
}

