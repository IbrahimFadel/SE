struct Vector<T> {
	buf T*,
	len u64,
	cap u64,
}

apply<T> to Vector<T> {
	fn new() -> This {
		Vector {
			buf: @flux.intrinsics.malloc(128) as T*,
			len: 0,
			cap: 128
		}
	}

	fn push(this Vector<T>, v T) {
		let buf = if this.len == this.cap {
			@flux.intrinsics.malloc(this.cap * 2) as T*
		} else {
			this.buf
		};

		let x s64 = 0;
		// buf[this.len] = v;
	}
}
